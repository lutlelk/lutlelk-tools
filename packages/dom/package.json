{
  "name": "@fe-utils/dom",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.global.js"
    },
    "./selector": {
      "types": "./dist/selector.d.ts",
      "import": "./dist/selector.js",
      "require": "./dist/selector.cjs"
    },
    "./class": {
      "types": "./dist/class.d.ts",
      "import": "./dist/class.js",
      "require": "./dist/class.cjs"
    },
    "./attr": {
      "types": "./dist/attr.d.ts",
      "import": "./dist/attr.js",
      "require": "./dist/attr.cjs"
    },
    "./style": {
      "types": "./dist/style.d.ts",
      "import": "./dist/style.js",
      "require": "./dist/style.cjs"
    },
    "./element": {
      "types": "./dist/element.d.ts",
      "import": "./dist/element.js",
      "require": "./dist/element.cjs"
    },
    "./event": {
      "types": "./dist/event.d.ts",
      "import": "./dist/event.js",
      "require": "./dist/event.cjs"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js",
      "require": "./dist/util.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "type-tests": "tsd"
  },
  "devDependencies": {
    "jsdom": "^27.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "DOM utilities including element selection, class manipulation, style management, and event handling",
  "keywords": [
    "dom",
    "element",
    "selector",
    "class",
    "style",
    "event",
    "attr",
    "utils",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1"
}
