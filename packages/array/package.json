{
  "name": "@lutlelk-tools/array",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.global.js"
    },
    "./chunk": {
      "types": "./dist/chunk.d.ts",
      "import": "./dist/chunk.js",
      "require": "./dist/chunk.cjs"
    },
    "./find": {
      "types": "./dist/find.d.ts",
      "import": "./dist/find.js",
      "require": "./dist/find.cjs"
    },
    "./flatten": {
      "types": "./dist/flatten.d.ts",
      "import": "./dist/flatten.js",
      "require": "./dist/flatten.cjs"
    },
    "./groupBy": {
      "types": "./dist/groupBy.d.ts",
      "import": "./dist/groupBy.js",
      "require": "./dist/groupBy.cjs"
    },
    "./sort": {
      "types": "./dist/sort.d.ts",
      "import": "./dist/sort.js",
      "require": "./dist/sort.cjs"
    },
    "./transform": {
      "types": "./dist/transform.d.ts",
      "import": "./dist/transform.js",
      "require": "./dist/transform.cjs"
    },
    "./uniq": {
      "types": "./dist/uniq.d.ts",
      "import": "./dist/uniq.js",
      "require": "./dist/uniq.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "type-tests": "tsd"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "description": "Array utilities including chunk, flatten, uniq, groupBy, sort, find, filter, map, reduce, and more",
  "keywords": [
    "array",
    "chunk",
    "flatten",
    "uniq",
    "groupBy",
    "sort",
    "find",
    "filter",
    "map",
    "reduce",
    "utils",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1"
}
