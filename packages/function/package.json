{
  "name": "@lutlelk/function",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.global.js"
    },
    "./debounce": {
      "types": "./dist/debounce.d.ts",
      "import": "./dist/debounce.js",
      "require": "./dist/debounce.cjs"
    },
    "./throttle": {
      "types": "./dist/throttle.d.ts",
      "import": "./dist/throttle.js",
      "require": "./dist/throttle.cjs"
    },
    "./memoize": {
      "types": "./dist/memoize.d.ts",
      "import": "./dist/memoize.js",
      "require": "./dist/memoize.cjs"
    },
    "./once": {
      "types": "./dist/once.d.ts",
      "import": "./dist/once.js",
      "require": "./dist/once.cjs"
    },
    "./partial": {
      "types": "./dist/partial.d.ts",
      "import": "./dist/partial.js",
      "require": "./dist/partial.cjs"
    },
    "./curry": {
      "types": "./dist/curry.d.ts",
      "import": "./dist/curry.js",
      "require": "./dist/curry.cjs"
    },
    "./compose": {
      "types": "./dist/compose.d.ts",
      "import": "./dist/compose.js",
      "require": "./dist/compose.cjs"
    },
    "./delay": {
      "types": "./dist/delay.d.ts",
      "import": "./dist/delay.js",
      "require": "./dist/delay.cjs"
    },
    "./retry": {
      "types": "./dist/retry.d.ts",
      "import": "./dist/retry.js",
      "require": "./dist/retry.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "type-tests": "tsd"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "description": "Function utilities including debounce, throttle, memoize, curry, compose, and more",
  "keywords": [
    "function",
    "utilities",
    "debounce",
    "throttle",
    "memoize",
    "curry",
    "compose",
    "pipe",
    "partial",
    "once",
    "retry"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1"
}
