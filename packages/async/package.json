{
  "name": "@fe-utils/async",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.global.js"
    },
    "./debounce": {
      "types": "./dist/debounce.d.ts",
      "import": "./dist/debounce.js",
      "require": "./dist/debounce.cjs"
    },
    "./throttle": {
      "types": "./dist/throttle.d.ts",
      "import": "./dist/throttle.js",
      "require": "./dist/throttle.cjs"
    },
    "./concurrency": {
      "types": "./dist/concurrency.d.ts",
      "import": "./dist/concurrency.js",
      "require": "./dist/concurrency.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "type-tests": "tsd"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "description": "Async utilities including debounce, throttle, and concurrency control",
  "keywords": [
    "async",
    "debounce",
    "throttle",
    "concurrency",
    "promise",
    "utils",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1"
}
